    "react-native-gesture-handler": "^2.28.0",
    "react-native-svg": "15.11.2",
    "react-native-svg-transformer": "^1.3.0",
    "react-native-web": "^0.20.0",
    "@expo/metro-runtime": "~5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@playwright/test": "^1.46.0",
